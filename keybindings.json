[
    {
        "key": "alt+j",
        "command": "cursorDown",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+k",
        "command": "cursorUp",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+h",
        "command": "cursorRight",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+l",
        "command": "cursorLeft",
        "when": "editorTextFocus"
    },
]
